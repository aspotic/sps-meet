/****Tag Configuration****/
body 
{
    background: #001C47;
}

img
{
    visibility: hidden;
}

/*links*/
a
{
	color: #DFBD0C;
}
a:hover
{
	color: #FFFFFF;
}


/*lists*/
li
{
	color: #6997D3;
}
li.title
{
	color: #4282D3;
}


/*forms*/
input,
select,
textarea
{
	color: #FFFF73;
	background:#27456E;
	border-color: #4282D3;
}
input.submit
{
	color: #FFFFFF;
	background:#274D7E;
	border-color:#27456E;
}


/*Tables*/
    div.tableTitle
    {
	    color: #B964D4;
	    background:#05326D;
	    border-color:#05326D;
    }
    table.tableList th 
    {
	    color: #D984F4;
	    border-color:#05326D;
	    background:#274D7E;
    }
    table.tableList td 
    {
	    color: #FFFFFF;
	    border-color:#05326D;
	    background:#375D8E;
    }


/****Universal dividers****/
div#pageTitle 
{
	background: #072C60;
	border-bottom:#27456E;
}
div#pageTitleText
{
	color:#B364C9;
}
div#navigation 
{
	background: #27456E;
	color: #FFFFFF;
}
div#navigation a
{
    color:#DFBD0C;
}
div#navigation a:hover
{
    color:#6997D3;
}


