/****Tag Configuration****/
body 
{
    background: #EBF7E1;
}
div#spacer
{  
    background: #FFFFFF;
}

/*links*/
a
{
	color: #537736;
}
a:hover
{
	color: #000000;
}


/*lists*/
ul
{
	color:#ffffff;
}
li
{
	color: #9CCC3C;
}
li.title
{
	color: #50720B;
}
/*forms*/
input,
select,
textarea
{
	color: #000000;
	background:#E2F5D2;
	border-color:#9CCC3C;
}
input.submit
{
	color: #50720B;
	background:#D7E9C8;
	border-color:#50720B;
}


/*Tables*/
table.tableList th 
{
	color: #50720B;
	border-color:#9CCC3C;
	background:#EBF7E1;
}
div.tableTitle
{
	color: #50720B;
	background:#EBF7E1;
	border-color:#9CCC3C;
}
table.tableList td 
{
	color: #000000;
	border-color:#9CCC3C;
	background:#EBF7E1;
}
    
    
/****Universal dividers****/
div#pageTitle 
{
	color:#3C651B;
	background: #EBF7E1;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#EBF7E1', endColorstr='#FFFFFF');
	background: -moz-linear-gradient(top,  #EBF7E1,  #FFFFFF);
}
div#pageTitleText
{
	color:#3C651B;
}
div#navigation 
{
	background: #FFFFFF;
}
div#body 
{
	background: #EBF7E1;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FFFFFF', endColorstr='#EBF7E1');
	background: -moz-linear-gradient(top,  #FFFFFF,  #EBF7E1);
}


