/****Tag Configuration****/
body 
{
    background: #FFA200;
}
img
{
    visibility: hidden;
}

/*links*/
a
{
	color: #006Ba2;
}
a:hover
{
	color: #000000;
}


/*lists*/
li
{
	color: #975BaA;
}
li.title
{
	color: #771B8A;
}


/*forms*/
input,
select,
textarea
{
	color: #FFFFFF;
	background:#470B5A;
	border-color: #470B5A;
}
input.submit
{
	color: #FFFFFF;
	background:#874B9A;
	border-color:#470B5A;
}


/*Tables*/
    table.tableList th 
    {
	    color: #000000;
	    border-color:#B35215;
	    background:#B35215;
    }
    div.tableTitle
    {
	    color: #000000;
	    background:#B35215;
	    border-color:#B35215;
    }
    table.tableList td 
    {
	    color: #000000;
	    border-color:#B35215;
	    background:#FFc240;
    }


/****Universal dividers****/
div#pageTitle 
{
	background: #571B6A;
